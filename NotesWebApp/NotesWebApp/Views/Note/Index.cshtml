@model List<NotesWebApp.Models.Entites.Note>

@{
    ViewData["Title"] = "Notes";
}

<div class="row">
    <div class="containerleftBar">
        <a asp-action="AddNote" class="btn btn-success btn-new-note">Add Note</a>

        <a asp-action="Search" class="btn btn-success btn-new-note">Search Note</a>

    </div>
    <div class="d-flex justify-content-around flex-wrap">
        @foreach (var item in Model)
        {
            @await Html.PartialAsync("_ViewNote.cshtml", item)
        }
    </div>
</div>
